,sl.[Profit _]
,sl.[Unit Price]
,sh.[Bill-to Name]
,sh.[Bill-to City]
,sh.[Order Date]
,cda.Description as discount
,sp.Name
,cp.Description as campaign
FROM [dbo].[CRONUS Polska Sp_ z o_o_$Sales Line] sl
JOIN [dbo].[CRONUS Polska Sp_ z o_o_$Sales Header] sh
ON(sl.[Document No_] = sh.No_)
JOIN [dbo].[CRONUS Polska Sp_ z o_o_$Customer] c
ON(sl.[Sell-to Customer No_] = c.No_)
LEFT JOIN [dbo].[CRONUS Polska Sp_ z o_o_$Customer Discount Group] cda
ON(c.[Customer Disc_ Group] = cda.Code)
JOIN [dbo].[CRONUS Polska Sp_ z o_o_$Salesperson_Purchaser] sp
ON(sh.[Salesperson Code] = sp.Code)
LEFT JOIN [dbo].[CRONUS Polska Sp_ z o_o_$Campaign] cp
ON(cp.[Salesperson Code] = sp.Code)</property>
